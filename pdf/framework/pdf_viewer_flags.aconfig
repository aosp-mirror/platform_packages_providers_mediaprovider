package: "android.graphics.pdf.flags"
container: "com.android.mediaprovider"

# All the flags that we are adding into Android V release have to be fixed read only.

flag {
    name: "enable_pdf_viewer"
    is_exported: true
    namespace: "pdf_viewer"
    description: "This flag controls whether to enable pdf viewer APIs."
    bug: "319561098"
    is_fixed_read_only: true
    is_exported: true
}

flag {
    name: "enable_form_filling"
    is_exported: true
    namespace: "pdf_viewer"
    description: "This flag controls whether to enable pdf viewer form filling APIs."
    bug: "319561098"
    is_fixed_read_only: true
    is_exported: true
}